        "custom_zqDYYW": {
          "type": "custom",
          "settings": {
            "code": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Product Information Tabs<\/title>\n    <link href=\"https:\/\/fonts.googleapis.com\/css2?family=Work+Sans:wght@400;500&display=swap\" rel=\"stylesheet\">\n    <style>\n        .tabbed-container {\n            font-family: 'Work Sans', sans-serif;\n            width: 100%;\n            margin-top: 20px;\n        }\n        .tab-links {\n            display: flex;\n            justify-content: start;\n            border-bottom: 2px solid #ccc;\n        }\n        .tab-link {\n            background: none;\n            border: none;\n            outline: none;\n            cursor: pointer;\n            padding: 10px 20px;\n            transition: background-color 0.3s;\n            font-size: 16px;\n            color: #333;\n            border-bottom: 2px solid transparent;\n            margin-bottom: -2px;\n        }\n        .tab-link:hover {\n            background-color: #ddd;\n        }\n        .tab-link.active {\n            font-weight: bold;\n            border-bottom: 2px solid black;\n        }\n        .tab-content {\n            display: none;\n            padding: 20px;\n            text-align: left;\n        }\n        .tab-content p {\n            margin: 0 0 10px 0;\n        }\n    <\/style>\n<\/head>\n<body>\n    <div class=\"tabbed-container\">\n        <!-- Tab Links -->\n        <div class=\"tab-links\">\n            <button class=\"tab-link active\" onclick=\"openTab(event, 'description')\">Product Details<\/button>\n            <button class=\"tab-link\" onclick=\"openTab(event, 'shipping')\">Shipping & Returns<\/button>\n            <button class=\"tab-link\" onclick=\"openTab(event, 'material')\">Material + Care<\/button>\n        <\/div>\n        <!-- Tab Content -->\n        <div id=\"description\" class=\"tab-content\" style=\"display: block;\">\n            <p>Crafted with pure ecru cotton and baroque French Valenciennes lace. Adjustable silk ribbons grace the shoulders and neckline, transforming effortlessly from mini to midi to maxi.<\/p>\n            <ul>\n                <li>3-in-1 Design<\/li>\n                <li>Pure Ecru Cotton<\/li>\n                <li>French Lace<\/li>\n                <li>Adjustable Ribbons<\/li>\n                <li>Shell Buttons<\/li>\n            <\/ul>\n            <p>Sophia is 5'5\" and wears a US-size XS.<\/p>\n        <\/div>\n        <div id=\"shipping\" class=\"tab-content\">\n            <p><strong>Shipping:<\/strong><\/p>\n            <p>Please allow 1-2 business days for processing and an additional 6-10 days for shipping. A tracking number will be provided.<\/p>\n            <p>NOTE: Due to high demand, we kindly ask you to allow up to 12 days to process and ship your order.<\/p>\n            <p>We offer Priority Shipping for 3 USD (7-10 Days) and Priority Expedited Shipping (5-7 Days) for 5 USD.<\/p>\n            <p>Free Standard Shipping on orders over $60.<\/p>\n            <p><strong>Returns:<\/strong><\/p>\n            <p>If you're not completely satisfied with your purchase, we're here to help. Our policy allows for returns within 30 days of delivery for a full refund.<\/p>\n        <\/div>\n        <div id=\"material\" class=\"tab-content\">\n            <p><strong>Material:<\/strong><\/p>\n            <ul>\n                <li>100% Semi-sheer Cotton (Dress)<\/li>\n                <li>100% Silk Ribbon (Straps)<\/li>\n            <\/ul>\n            <p><strong>Care:<\/strong><\/p>\n            <p>Hand wash cold and hang to dry<\/p>\n        <\/div>\n    <\/div>\n    <script>\n        function openTab(evt, tabName) {\n            var i, tabcontent, tablinks;\n            tabcontent = document.getElementsByClassName(\"tab-content\");\n            for (i = 0; i < tabcontent.length; i++) {\n                tabcontent[i].style.display = \"none\";\n            }\n            tablinks = document.getElementsByClassName(\"tab-link\");\n            for (i = 0; i < tablinks.length; i++) {\n                tablinks[i].className = tablinks[i].className.replace(\" active\", \"\");\n            }\n            document.getElementById(tabName).style.display = \"block\";\n            evt.currentTarget.className += \" active\";\n        }\n    <\/script>\n<\/body>\n<\/html>"
          }
        },
        "custom_Qj8UYj": {
          "type": "custom",
          "disabled": true,
          "settings": {
            "code": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n<meta charset=\"UTF-8\">\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n<title>Money Back Guarantee<\/title>\n<style>\n  .money-back-container {\n    display: flex;\n    align-items: flex-start; \/* Aligns items to the start of the container *\/\n    padding-left: 0; \/* Removes padding on the left for full left alignment *\/\n    text-align: left; \/* Ensures text within the container is left-aligned *\/\n  }\n  .money-back-svg {\n    margin-right: 10px; \/* Adjust the space between the SVG and the text *\/\n    width: 30px; \/* Width of the SVG image *\/\n    height: auto; \/* Keeps the aspect ratio of the SVG image *\/\n  }\n  .money-back-text {\n    font-size: 14px; \/* Size of the text *\/\n    color: #333; \/* Color of the text *\/\n    margin-top: 3px; \/* Adjusts the text to align properly with the SVG *\/\n  }\n<\/style>\n<\/head>\n<body>\n\n<div class=\"money-back-container\">\n  <!-- SVG image -->\n  <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0587\/4132\/5898\/files\/30-days.png?v=1714171015\" alt=\"30 Days\" class=\"money-back-svg\">\n\n  <!-- Text -->\n  <div class=\"money-back-text\">\n    Less than 1% of customers claim our Money Back Guarantee\n  <\/div>\n<\/div>\n\n<\/body>\n<\/html>"
